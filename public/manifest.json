{
  "manifest_version": 3,
  "name": "Actively watch YouTube",
  "description": "Not block YouTube channels.",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "runAt": "document_idle",
      "matches": ["https://example.com/"],
      "js": ["assets/test.js"]
    }
  ]
}
